---
// styles
import "../styles/headings.css";
import "../styles/global.css";
import "../styles/reset.css";

// components
import Navbar from "../components/navbar/Navbar.astro";

interface Props {
  title: string;
}

const { title } = Astro.props;
---

<!doctype html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />

    <meta
      name="description"
      content="Ofrecemos servicios dentales de calidad en Escárcega. Ortodoncia, blanqueamiento dental y más. ¡Agenda tu cita hoy!"
    />
    <meta
      name="keywords"
      content="dentista Escárcega, ortodoncia, blanqueamiento dental, clínica dental"
    />
    <meta name="author" content="consultorio acosta" />
    <meta name="robots" content="index, follow" />
    <meta name="geo.position" content="18.60870946007853, -90.72663345852085" />
    <meta name="geo.placename" content="Escárcega" />
    <meta name="geo.region" content="MX-CAM" />
    <meta property="og:title" content="Consultorio dental Acosta" />
    <meta
      property="og:description"
      content="Ofrecemos servicios dentales de calidad en Escárcega. Ortodoncia, blanqueamiento dental y más. ¡Agenda tu cita hoy!"
    />
    <meta property="og:image" content="/assets/team-rosa-pro.webp" />

    <link rel="apple-touch-icon" sizes="180x180" href="/apple-touch-icon.png" />
    <link rel="icon" type="image/png" sizes="32x32" href="/favicon-32x32.png" />
    <link rel="icon" type="image/png" sizes="16x16" href="/favicon-16x16.png" />
    <link rel="manifest" href="/site.webmanifest" />

    <meta name="generator" content={Astro.generator} />
    <title>{title}</title>
  </head>
  <body>
    <Navbar />
    <main>
      <slot />
    </main>
    <script src="https://cdn.botpress.cloud/webchat/v1/inject.js" is:inline
    ></script>
    <script
      src="https://mediafiles.botpress.cloud/ba57d301-1b37-4aa2-b0bc-4cffc632a9b1/webchat/config.js"
      defer
      is:inline></script>
  </body>
</html>
